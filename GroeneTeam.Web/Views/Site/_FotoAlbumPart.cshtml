@model FotoAlbumPart

@{
    var contentPartTaal = (ContentPartTaal)ViewData["contentPartTaal"]; 
    var contentPagina = (ContentPagina)ViewData["ContentPagina"];
    var contentPart = new ContentPart(Model, contentPagina);    
    var mode = (Mode)ViewData["Mode"];
}

@switch (mode) {
    case Mode.Bewerk: 
        @Html.FotoAlbumBewerkPartial(contentPartTaal)
        break;
    case Mode.Web:
        if(contentPart.ContentPaginaKolom.IsHoofdKolom){
            @Html.FotoAlbumPartBekijkPartial(Model, contentPagina, 350, 75, 75, "FotoAlbum", 12, true)
        }else{
            @Html.FotoAlbumPartBekijkPartial(Model, contentPagina, 185, 75, 75, "FotoAlbum", 6, true)
        }
        break;   
}