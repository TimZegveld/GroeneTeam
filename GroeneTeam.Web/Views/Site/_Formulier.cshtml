@model Formulier

@{
    var contentPartTaal = (ContentPartTaal)ViewData["contentPartTaal"];
    var contentPagina = (ContentPagina)ViewData["ContentPagina"];
    var mode = (Mode)ViewData["Mode"];
}

@switch (mode){
    case Mode.Bewerk:
        @Html.GestructureerdeDataBewerkPartial(contentPartTaal, Model)
        break;
    case Mode.Web:
        @Html.FormulierBekijkPartial(Model, contentPagina)
        break;
}