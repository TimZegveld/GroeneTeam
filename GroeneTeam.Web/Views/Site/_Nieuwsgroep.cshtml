@model NieuwsGroep

@{
    var contentPartTaal = (ContentPartTaal)ViewBag.contentPartTaal;
    var contentPagina = (ContentPagina)ViewBag.ContentPagina;
    var mode = (Mode)ViewBag.Mode;
}

@switch (mode){        
    case Mode.Bewerk:
        @Html.NieuwsGroepBewerkPartial(contentPartTaal)
        break;
    case Mode.Web:
        @Html.NieuwsGroepBekijkPartial(Model)
        break;
    default:
        break;    
}